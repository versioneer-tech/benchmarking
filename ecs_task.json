{
  "family": "analysis-sample-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "2048",
  "executionRoleArn": "arn:aws:iam::651706767831:role/ecsTaskExecutionRole",
  "runtimePlatform": {
    "cpuArchitecture": "ARM64",
    "operatingSystemFamily": "LINUX"
  },
  "containerDefinitions": [
    {
      "name": "analysis-sample",
      "image": "651706767831.dkr.ecr.eu-west-2.amazonaws.com/earthcode/pangeo-example@sha256:76482919cd7045700d07092feb9b5825940c38335f09505158833d9c96d02b4d",
      "essential": true,
      "environment": [
        { "name": "URL", "value": "https://eoresults.esa.int/e/earthcode/seasfire_tests/seasfire_v0.4.zarr"},
        { "name": "LARGE_JOB", "value": "1" },
        { "name": "NUM_RUNS", "value": "1" },
        { "name": "RUN_ID", "value": "eoreslarge" },
        { "name": "POD_NAME", "value": "na" },
        { "name": "WAIT_TIME", "value": "5" }
